    "node-fetch": "^2.3.0",
    "rxjs": "^6.3.0",
    "tar-fs": "^1.16.3",
    "tar-stream": "^1.6.1"
  },
  "devDependencies": {
    "flow-bin": "^0.88.0"
  }
}
